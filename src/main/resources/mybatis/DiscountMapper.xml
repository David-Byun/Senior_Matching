<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kbstar.mapper.DiscountMapper">
    <select id="getDiscount" parameterType="String" resultType="Discount">
        SELECT *
        FROM discount
        WHERE code = #{code}
    </select>
</mapper>